{
  "name": "ngx-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:es5": "ng serve --proxy-config proxy.conf.json --configuration es5",
    "start:prod": "ng serve --proxy-config proxy.conf.json --configuration production",
    "build": "npm run lint && ng build",
    "build:prod": "npm run lint && ng build --configuration production",
    "build:bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/ngx-starter/stats-es2015.json",
    "build:bundle-report:prod": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/ngx-starter/stats-es2015.json",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage",
    "lint": "ng lint && stylelint './src/**/*.scss'",
    "lint:fix": "ng lint --fix && stylelint './src/**/*.scss' --fix",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/localize": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ng-select/ng-select": "~8.1.1",
    "@ngneat/until-destroy": "~9.0.0",
    "bootstrap": "4.3.1",
    "font-awesome": "4.7",
    "jquery": "3.5",
    "lodash": "~4.17.21",
    "luxon": "~2.2.0",
    "ngx-bootstrap": "~7.1.2",
    "popper.js": "1.16",
    "roboto-fontface": "0.10",
    "rxjs": "^7.4.0",
    "socket.io-client": "^2.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "4.14",
    "@types/luxon": "^1.27.1",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "1.4",
    "@types/webpack": "4.41",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "1.2",
    "lint-staged": "~12.1.2",
    "prettier": "~2.5.1",
    "protractor": "~7.0.0",
    "stylelint": "13.8",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-recommended-scss": "4.2",
    "stylelint-prettier": "1.1",
    "stylelint-scss": "3.18",
    "ts-node": "~8.3.0",
    "typescript": "~4.5.4",
    "webpack-bundle-analyzer": "3.3.2",
    "husky": "^7.0.0"
  },
  "lint-staged": {
    "*.{scss, css}": "stylelint --fix",
    "*.ts": "eslint --fix",
    "*.html": "eslint --fix"
  }
}
