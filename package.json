{
  "name": "ngx-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:es5": "ng serve --proxy-config proxy.conf.json --configuration es5",
    "start:prod": "ng serve --proxy-config proxy.conf.json --configuration production",
    "build": "npm run lint && ng build",
    "build:prod": "npm run lint && ng build --configuration production",
    "build:bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/ngx-starter/stats-es2015.json",
    "build:bundle-report:prod": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/ngx-starter/stats-es2015.json",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage",
    "lint": "ng lint && stylelint './src/**/*.scss'",
    "lint:fix": "ng lint --fix && stylelint './src/**/*.scss' --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.14",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/localize": "~12.2.14",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@ng-select/ng-select": "~7.4.0",
    "@ngneat/until-destroy": "~8.1.4",
    "bootstrap": "4.3.1",
    "font-awesome": "4.7",
    "jquery": "3.5",
    "lodash": "~4.17.21",
    "luxon": "~2.2.0",
    "ngx-bootstrap": "~7.1.2",
    "popper.js": "1.16",
    "roboto-fontface": "0.10",
    "rxjs": "~6.6.0",
    "socket.io-client": "^2.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.14",
    "@angular/language-service": "~12.2.14",
    "@types/jasmine": "~3.8.0",
    "@types/lodash": "4.14",
    "@types/luxon": "^1.27.1",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "1.4",
    "@types/webpack": "4.41",
    "codelyzer": "^6.0.0",
    "husky": "4.3",
    "jasmine-core": "~3.6.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "1.2",
    "lint-staged": "10.5",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "stylelint": "13.8",
    "stylelint-config-prettier": "8.0",
    "stylelint-config-recommended-scss": "4.2",
    "stylelint-prettier": "1.1",
    "stylelint-scss": "3.18",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18",
    "tslint-plugin-prettier": "2.3",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{scss, css}": "stylelint --fix",
    "*.ts": "prettier --write",
    "*.html": "prettier --write"
  }
}
